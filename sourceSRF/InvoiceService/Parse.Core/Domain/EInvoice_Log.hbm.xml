<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Parse.Core.Domain.EInvoice_Log,Parse.Core" table="EInvoice_Log" dynamic-update="true">
    <id name="Id" column="Id" type="decimal">
      <generator class="identity" />
    </id>
    <property name="DocEntry" column="DocEntry" type="int"/>
    <property name="Serial" column="Serial" type="string" length="20"/>
    <property name="Pattern" column="Pattern" type="string" length="20"/>
    <property name="InvNo" column="InvNo" type="string" length="30"/>
    <property name="IssueDate" column="IssueDate" type="DateTime" />
    <property name="CreatedDate" column="CreatedDate" type="DateTime"/>
    <property name="ErrorCode" column="ErrorCode" type="string" length="50"/>
    <property name="ErrorDesc" column="ErrorDesc" type="string" length="1000"/>
  </class>
</hibernate-mapping>
